/*
	This C Flat program shows off various arithmetic operators,
	their prececence and associativity
*/

typedef int x;
typedef x otherint;

int main() {
	otherint a=0,b,c,d;
	b = c = ++a * (6%4 + a);
	a++;
	a--;
	d = (14 / (sizeof(b)/2))+(80*12)/(80*12)-(!0);
	puti(a);
	putc('\n');
	puti(b);
	putc('\n');
	puti(c);
	putc('\n');
	puti(d);
	putc('\n');
	putc('\n');
	return 0;
}
