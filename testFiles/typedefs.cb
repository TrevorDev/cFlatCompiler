int factorial(int n)
{
	int ret;

	puti(n);
	putc(' ');

	if (n > 0) {
		ret = factorial(n - 1) * n;
	}
	else {
		ret = 1;
	}
	return ret;
}

int fucked_factorial(int n)
{
	int ret;

	puti(n);
	putc(' ');

	if (n <= 0) {
		ret = 1;
	}
	else {
		putc(' ');
		puti(n);
		fucked_factorial(n - 1);
		ret = fucked_factorial(n - 1) * n;
	}
	return ret;
}

int main()
{
	puti(fucked_factorial(5));
    return 1;
}