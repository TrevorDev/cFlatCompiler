/*Multiplication table*/
int main()
{
	int x;
	int j;
	int size = 3;
	putc(' ');
	putc(' ');
	putc(' ');
	for(x = 1;x <= size;x++){
		puti(x);
		putc(' ');
		putc(' ');
	}
	putc('\n');
	putc(' ');
	putc(' ');
	putc(' ');
	for(x = 1;x <= size;x++){
		putc('_');
		putc(' ');
		putc(' ');
	}
	putc('\n');
	for(x = 1;x <= size;x++){
		puti(x);
		putc(' ');
		putc('|');
		j = 1;
		while(j <= size){
			puti(j*x);
			putc(' ');
			putc(' ');
			j++;
		}
		putc('\n');
	}	
    return 1;
}